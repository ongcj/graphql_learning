type Query {
    hello(who: String): String!
    people: [Person!]!
    employee(id: ID): Employee!
}

type Mutation {
    createPerson(firstName: String!, lastName: String!, dateOfBirth: Date): Person!
}

scalar Date

type Person {
    id: Int!
    firstName: String!
    lastName: String!
    dateOfBirth: Date
}

type Employee {
    businessentityid: ID!
    loginid: String!
    jobtitle: String!
}
